package com.app;

import java.util.HashMap;

public class HashMapExample {

	public static void main(String[] args) {

		//Create a HashMap 
        HashMap<String,Integer> map=new HashMap<>();
       
		//Add key-value pairs to HashMap
        
        map.put("Mickey", 30);
        map.put("Oliver", 25);
        map.put("George", 20);
		
		//Access a value in HashMap
        int age=map.get("Mickey");
        System.out.println("Mickey's age is: " + age);
		
		
		//Check if HashMap contains a key
        boolean hasOliver=map.containsKey("Oliver");
        System.out.println("Hashmap contains Oliver: " + hasOliver);
		
		
		//Check if HashMap contains a value
        
        boolean hasAge25=map.containsValue(25);
        System.out.println("Hashmap contains 25: " + hasAge25);

		
		//Remove a key value pair from HashMap
        
        map.remove("George");
        System.out.println("After removing George: "+ map);
		
		//Get size of HashMap
        
        int size=map.size();
        System.out.println("HashMap Size: "+ size);
		
		//Clear all values in HashMap
        map.clear();
        System.out.println("HashMap Size: "+ map);


	}

}
